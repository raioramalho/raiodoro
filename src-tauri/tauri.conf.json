{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": "v1Compatible"
  },
  "productName": "raiodoro",
  "version": "0.1.3",
  "identifier": "br.com.raiosystems",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "R⚡️",
        "width": 300,
        "height": 300,
        "resizable": false,
        "maximizable": false,
        "titleBarStyle": "Transparent",
        "decorations": true,
        "transparent": false
      }
    ],
    "macOSPrivateApi": false
  }
}